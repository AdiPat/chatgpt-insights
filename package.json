{
  "name": "chatgpt-insights",
  "version": "1.0.0",
  "description": "ChatGPT Insights Project",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "chatgpt-insights": "./dist/index.js"
  },
  "scripts": {
    "start": "NODE_OPTIONS='--loader ts-node/esm --experimental-specifier-resolution=node' node src/index.ts",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "vitest run --coverage",
    "test:watch": "vitest --coverage",
    "dev": "NODE_OPTIONS='--loader ts-node/esm --experimental-specifier-resolution=node' node --watch src/index.ts"
  },
  "keywords": [
    "chatgpt",
    "insights",
    "typescript",
    "cli"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^11.1.0",
    "extract-zip": "^2.0.1",
    "inquirer": "^9.2.15",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.24",
    "@vitest/coverage-v8": "^1.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
